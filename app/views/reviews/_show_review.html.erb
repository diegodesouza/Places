<% @reviews.each do |review| %>
  <%= review.title %>
  <%= review.description %>
<% end %>

<% if review.user == current_user %>
  <div> <%= link_to "Edit", edit_listing_review_path(@listing, review)  %> </div>
  <div> <%= link_to "Delete Review", listing_review_path(@listing, review), method: :delete %> </div>
<% end %>
