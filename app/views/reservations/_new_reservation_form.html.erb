<%= form_for [@listing, @reservation] do |f| %>
  <%= f.label :check_in, "Check In" %>
  <%= f.text_field :check_in, placeholder: "MM/DD/YYYY" %>

  <%= f.label :check_out, "Check Out" %>
  <%= f.text_field :check_out, placeholder: "MM/DD/YYYY" %>

  <%= f.submit "Submit", method: :post %>
<% end %>
